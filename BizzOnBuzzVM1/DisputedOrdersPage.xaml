<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BizzOnBuzzVM1.DisputedOrdersPage"
             Title="DisputedOrdersPage">
    <Grid Margin="0,25,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        
        <Label Text="Customer Name"
               Grid.Column="0"
               HorizontalOptions="Center"/>
        
        <Label Text="Customer Order"
               Grid.Column="1"
               HorizontalOptions="Center"/>
        
        <Label Text="Customer Email"
               Grid.Column="2"
               HorizontalOptions="Center"/>
        
        <Label Text="Dispute"
               Grid.Column="3"
               HorizontalOptions="Center"/>
        
        <Label Text="Resolution"
               Grid.Column="4"
               HorizontalOptions="Center"/>

        <ImageButton
            Grid.Column="0"
         Source="home_icon.png"
         HeightRequest="70"
         Margin="0,900,60,0"
            Clicked="ImageButton_Clicked"/>
       
        <Label Text="Home"
               Grid.Column="0"
               Margin="40,1010,0,0"/>








    </Grid>
</ContentPage>